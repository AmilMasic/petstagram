<%= form_for (@pet) do |f| %>
  <%= f.label :name %>
  <br>
  <%= f.text_field :name %>
  <br>
  <br>
  <%= f.label :breed %>
  <br>
  <%= f.text_field :breed %>
  <br>
  <br>
  <%= f.label :age %>
  <br>
  <%= f.text_field :age %>
  <br>
  <%= f.file_field :images, multiple: true %>
  <br>

  <%= f.submit "Submit" %>
<% end %>
